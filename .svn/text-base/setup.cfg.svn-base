[egg_info]
tag_build = dev
tag_svn_revision = true

[global]
command_packages = buildutils.pudge_command

[extract_messages]
output_file = darengs/i18n/darengs.pot
add_comments = TRANSLATORS:
width = 80

[init_catalog]
domain = darengs
output_dir = darengs/i18n
input_file = darengs/i18n/darengs.pot

[compile_catalog]
directory = darengs/i18n
domain = darengs
statistics = true

[easy_install]
find_links = http://www.pylonshq.com/download/

[update_catalog]
domain = darengs
previous = true
output_dir = darengs/i18n
input_file = darengs/i18n/darengs.pot

